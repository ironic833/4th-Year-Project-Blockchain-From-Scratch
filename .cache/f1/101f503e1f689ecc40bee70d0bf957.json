 === 24) {\r\n    columns = 2;\r\n  } else if (words.length > 24) {\r\n    columns = Math.ceil(words.length / 12);\r\n  }\r\n\r\n  const columnSize = Math.floor(12 / columns);\r\n\r\n  const columnItems = [];\r\n  for (let i = 0; i < columns; i++) {\r\n    const startIndex = i * columnSize;\r\n    const endIndex = Math.min((i + 1) * columnSize, words.length);\r\n    const columnWords = words.slice(startIndex, endIndex);\r\n    columnItems.push(\r\n      <div key={i} className=\"column\">\r\n        {columnWords.join(' ')}\r\n      </div>\r\n    );\r\n  }\r\n\r\n  return (\r\n    <div className=\"wallet-phrase\">\r\n      {columnItems}\r\n    </div>\r\n  );\r\n}\r\n\r\nexport default WalletMnemonic;\r\n"},"lineCount":null}},"error":null,"hash":"431d4af94ee5c8addc209e01b07bbec5","cacheData":{"env":{}}}