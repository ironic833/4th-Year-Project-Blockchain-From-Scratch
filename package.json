{
  "name": "auctionchain",
  "productName": "Auction Chain",
  "version": "2.0.0",
  "description": "Auction chain is a blockchain based auction system",
  "main": "index.js",
  "build": {
    "appId": "Hickey.AuctionChain",
    "win": {
      "target": "portable",
      "icon": "client/src/assets/icon.png"
    }
  },
  "scripts": {
    "test": "jest --watchAll",
    "start": "electron index.js",
    "rebuild-client": "npm run dev-client",
    "dev-client": "npm run clean && parcel client/src/index.html --out-dir client/dist",
    "clean": "if exist (rmdir /s/q .cache client/dist)",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "postinstall": "electron-builder install-app-deps",
  "jest": {
    "testEnvironment": "node"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "cross-env": "^5.2.0",
    "electron": "^24.0.0",
    "electron-builder": "^23.6.0",
    "jest": "^29.3.1",
    "nodemon": "^1.18.4"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.7",
    "bip39": "^3.1.0",
    "body-parser": "^1.18.3",
    "bootstrap": "^5.2.3",
    "elliptic": "^6.4.1",
    "express": "^4.16.3",
    "hdkey": "^2.1.0",
    "hex-to-binary": "^1.0.1",
    "history": "^4.7.2",
    "parcel-bundler": "^1.12.5",
    "pubnub": "^4.21.6",
    "react": "^18.2.0",
    "react-bootstrap": "^2.7.2",
    "react-copy-to-clipboard": "^5.1.0",
    "react-dom": "^18.2.0",
    "react-router-bootstrap": "^0.25.0",
    "react-router-dom": "^4.3.1",
    "request": "^2.88.0",
    "uuid": "^3.3.2"
  }
}
